{
  "version": 2,
  "builds": [
    {
      "src": "routes/**/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/routes"
    },
    {
      "src": "/addPost",
      "dest": "/routes/addPost"
    }
  ],
  "env": {
    "DASECRET": "@dasecret",
    "MONGO_DB_URL": "@mongo_db_url",
    "HASHKEY": "@hash_key"
  }
}